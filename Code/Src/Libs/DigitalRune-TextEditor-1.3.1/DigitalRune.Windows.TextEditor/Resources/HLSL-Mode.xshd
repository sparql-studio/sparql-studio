    <RuleSet name = "TechniquesAndPassesSet" ignorecase = "false" highlightDigits = "true">
      <Delimiters>&amp;&lt;&gt;~!%^*()-+=|\/{}[]:;"' , .?</Delimiters>

      <Span name = "LineComment" rule = "CommentMarkerSet" bold = "false" italic = "false" color = "Green" stopateol = "true">
        <Begin>//</Begin>
      </Span>

      <Span name = "BlockComment" rule = "CommentMarkerSet" bold = "false" italic = "false" color = "Green" stopateol = "false">
        <Begin>/*</Begin>
        <End>*/</End>
      </Span>

      <Span name = "pass" rule = "TechniquesAndPassesSet" bold = "false" italic = "false" color = "Black" stopateol = "false">
        <Begin singleword = "true" bold = "false" italic = "false" color = "BlueViolet">pass</Begin>
        <End>}</End>
      </Span>

      <Span name = "pass" rule = "TechniquesAndPassesSet" bold = "false" italic = "false" color = "Black" stopateol = "false">
        <Begin singleword = "true" bold = "false" italic = "false" color = "BlueViolet">Pass</Begin>
        <End>}</End>
      </Span>

      <!-- 
        Some effect files (mainly CgFX files) sometimes use array initializers for effect states, e.g. "ColorMask = { 1, 1, 1, 1 };"
        But we assume that a '}' closes the current pass.
        To ignore such initializers, we add the following rule.
      -->
      <Span name = "Initializer1" rule = "TechniquesAndPassesSet" bold = "false" italic = "false" color = "Black" stopateol = "true">
        <Begin>={</Begin>
        <End>}</End>
      </Span>

      <Span name = "Initializer2" rule = "TechniquesAndPassesSet" bold = "false" italic = "false" color = "Black" stopateol = "true">
        <Begin>= {</Begin>
        <End>}</End>
