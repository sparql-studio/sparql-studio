<?xml version="1.0"?>
<!-- Syntax definition for HLSL and DirectX Effect Files by Martin Garstenauer -->

<SyntaxDefinition name = "HLSL" extensions = ".fx;.fxh;.hlsl">

  <Environment>
    <Custom name="TypeReference" bold="false" italic="false" color="#04ABAB" />
    <Custom name="UnknownEntity" bold="false" italic="false" color="#AB0404" />
  </Environment>

  <Properties>
    <Property name="LineComment" value="//"/>
    <Property name="BlockCommentBegin" value="/*"/>
    <Property name="BlockCommentEnd" value="*/"/>
  </Properties>

  <Digits name = "Digits" bold = "false" italic = "false" color = "DarkRed"/>

  <RuleSets>
    <RuleSet ignorecase="false">
      <Delimiters>&amp;&lt;&gt;~!%^*()-+=|\/{}[]:;"' , .?</Delimiters>

      <Span name = "LineComment" rule = "CommentMarkerSet" bold = "false" italic = "false" color = "Green" stopateol = "true">
        <Begin>//</Begin>
      </Span>

      <Span name = "BlockComment" rule = "CommentMarkerSet" bold = "false" italic = "false" color = "Green" stopateol = "false">
        <Begin>/*</Begin>
        <End>*/</End>
      </Span>

      <Span name = "include" rule = "IncludeStatemetSet" bold = "false" italic = "false" color = "Black" stopateol = "true" escapecharacter="\">
        <Begin startofline="true" color = "Blue">#include</Begin>
      </Span>

      <Span name = "technique" rule = "TechniquesAndPassesSet" bold = "false" italic = "false" color = "Black" stopateol = "false">
        <Begin singleword = "true" bold = "false" italic = "false" color = "BlueViolet">technique</Begin>
        <End>}</End>
      </Span>

      <Span name = "technique" rule = "TechniquesAndPassesSet" bold = "false" italic = "false" color = "Black" stopateol = "false">
        <Begin singleword = "true" bold = "false" italic = "false" color = "BlueViolet">Technique</Begin>
        <End>}</End>
      </Span>

      <Span name = "technique10" rule = "TechniquesAndPassesSet" bold = "false" italic = "false" color = "Black" stopateol = "false">
        <Begin singleword = "true" bold = "false" italic = "false" color = "BlueViolet">technique10</Begin>
        <End>}</End>
      </Span>

      <Span name = "sampler_state" rule = "TechniquesAndPassesSet" bold = "false" italic = "false" color = "Black" stopateol = "false">
        <Begin singleword = "true" bold = "false" italic = "false" color = "Blue">sampler_state</Begin>
        <End>}</End>
      </Span>

      <Span name = "SamplerState" rule = "TechniquesAndPassesSet" bold = "false" italic = "false" color = "Black" stopateol = "false">
        <Begin singleword = "true" bold = "false" italic = "false" color = "Blue">SamplerState</Begin>
        <End>}</End>
      </Span>

      <Span name = "SamplerComparisonState" rule = "TechniquesAndPassesSet" bold = "false" italic = "false" color = "Black" stopateol = "false">
        <Begin singleword = "true" bold = "false" italic = "false" color = "Blue">SamplerComparisonState</Begin>
        <End>}</End>
      </Span>

      <Span name = "StateBlock" rule = "TechniquesAndPassesSet" bold = "false" italic = "false" color = "Black" stopateol = "false">
        <Begin singleword = "true" bold = "false" italic = "false" color = "Blue">stateblock_state</Begin>
        <End>}</End>
      </Span>

      <Span name = "BlendState" rule = "TechniquesAndPassesSet" bold = "false" italic = "false" color = "Black" stopateol = "false">
        <Begin singleword = "true" bold = "false" italic = "false" color = "Blue">BlendState</Begin>
        <End>}</End>
      </Span>

      <Span name = "DepthStencilState" rule = "TechniquesAndPassesSet" bold = "false" italic = "false" color = "Black" stopateol = "false">
        <Begin singleword = "true" bold = "false" italic = "false" color = "Blue">DepthStencilState</Begin>
        <End>}</End>
      </Span>

      <Span name = "RasterizerState" rule = "TechniquesAndPassesSet" bold = "false" italic = "false" color = "Black" stopateol = "false">
        <Begin singleword = "true" bold = "false" italic = "false" color = "Blue">RasterizerState</Begin>
        <End>}</End>
      </Span>

      <Span name = "asm" rule = "AssemblerSet" bold = "false" italic = "false" color = "Black" stopateol = "false">
        <Begin singleword = "true" bold = "false" italic = "false" color = "Blue">asm</Begin>
        <End>}</End>
      </Span>

      <Span name = "String" bold = "false" italic = "false" color = "DarkRed" stopateol = "true" escapecharacter="\">
        <Begin>"</Begin>
        <End>"</End>
      </Span>

      <Span name = "Char" bold = "false" italic = "false" color = "DarkRed" stopateol = "true" escapecharacter="\">
        <Begin>&apos;</Begin>
        <End>&apos;</End>
      </Span>

      <!-- Functions 
      <MarkPrevious bold = "true" italic = "false" color = "MidnightBlue">(</MarkPrevious>
      -->

      <!-- Semantics -->
      <MarkFollowing bold = "true" italic = "false" color = "Gray">:</MarkFollowing>

      <KeyWords name = "Punctuation" bold = "false" italic = "false" color = "Black">
        <Key word = "?" />
